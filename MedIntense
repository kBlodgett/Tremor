import java.util.*;
public class MedIntense
{
   private List<DataPerMin>  medTremor = new ArrayList<DataPerMin>();

    /**
     * Constructor for objects of class LowIntense
     */
    public MedIntense(List tremorList)
    {
        medTremor = tremorList;
    }

    /**
     * prints out the array list of lowest intensity tremors
     */
    public void print()
    {
        DataPerMin med;
        //System.out.printf("%-10s %-10s %-5s %-20s %n","Intensity", "End Time", "HR", "Vector Magnitude");
        for(int i = 0; medTremor.size() > i; i++)
        {
            med = (DataPerMin) medTremor.get(i);
            System.out.printf("%-10s %-10s %-5s %-20s %n","Medium", med.time(), med.HR(), med.vec());
        }
        
    }
}
